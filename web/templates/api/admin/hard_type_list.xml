<?xml version="1.0" encoding="windows-1251"?>
<info count='{{count}}'>
    {% for row in data %}
    <type>
    	<id>{{row.id}}</id>
        <name>{{row.name}}</name>
    	<hard_id>{{row.hard_id}}</hard_id>
    	<color_id>{{row.color_id}}</color_id>
    	<pattern_id>{{row.pattern_id}}</pattern_id>
        <sort>{{row.sort}}</sort>
    </type>
    {% endfor %}
</info>
